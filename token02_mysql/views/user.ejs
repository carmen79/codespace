<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>

  
</head>

<body>
<ul id = "list">

    <li>INNER TEXTSLDFALDKFALDFLADFLDALDKF</li>
</ul>
   
</body>

<script>
    const token = localStorage.getItem("token")
    fetch ("api/users", {
        headers:{
            "Content-type": "application/json",
            "Authorization": "Bearer " + token
        }

    }).then(res =>{     
        if(res.ok) {
            res.json().then(json =>
            // es json.map es como un for que recorre las filas que vienen en el body del response
                json.map(userRegister => { // userRegister representa una fila en cada iteraci√≥n del map
                    let myLiElement = document.createElement("li"); //"li" es el tipo de elemento html que voy a crear
                    if (userRegister.email != null){
                        myLiElement.innerText = userRegister.username +" ( " + userRegister.email + ")";
                    }else {
                        myLiElement.innerText = userRegister.username; 
                    }
            
                    document.getElementById("list").appendChild(myLiElement);
                })
            )
        }
    })

</script>

</html>